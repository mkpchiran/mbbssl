<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.org.chiranz.aitem">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/main_activity_result">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ResultActivity"
            android:label="@string/title_activity_result"
            android:parentActivityName=".MainActivity" />

        <meta-data
            android:name="io.fabric.ApiKey"
            android:value="ead4de5e8e115ee67eee92db85493763a74bbd74" />

        <activity
            android:name=".DoctorActivity"
            android:label="@string/title_activity_person"
            android:parentActivityName=".ResultActivity" ></activity>
    </application>

</manifest>